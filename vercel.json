{
  "version": 2,
  "name": "ramen-app-final",
  "regions": ["hnd1"],
  "functions": {
    "api/**/*.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/app"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
